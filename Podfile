platform :ios, '10.0'
use_frameworks!

target ‘SSUMAP_iOS’ do
  pod 'Alamofire', '~> 4.4'
  pod 'Kanna', '~> 2.1.0'
  pod 'Kingfisher', '~> 3.0'
  pod 'GoogleMaps'
  pod 'GooglePlaces'
end

  target 'SSUMAP_iOSTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SSUMAP_iOSUITests' do
    inherit! :search_paths
    # Pods for testing
  end
